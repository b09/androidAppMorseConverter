<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.macbook.mymenuproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.morse.AlphaToMorseActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activities.morse.MorseToAlphaActivity"
            android:label="@string/app_name" />
        <activity android:name=".activities.binary.AlphaToBinaryActivity" />
        <activity android:name=".activities.binary.BinaryToAlphaActivity" />
        <activity android:name=".activities.hebrew.AlphaToHebrewActivity" />
        <activity android:name=".activities.hebrew.HebrewToAlphaActivity" />
    </application>

</manifest>